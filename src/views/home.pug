doctype html
html(lang="en")
    head
        title Administracion de productos
        include includes/head.pug
    body(style="padding: 16px")
        div(class="container", id="user")
        div(class="container")
            div(class="row")
                div(class="col-lg-12 col-sm-12 position-relative", id="chat-container")
                    div(class="row d-flex justify-content-center")
                        div(class="col-lg-6 col-sm-12 col-md-6 ")
                            include includes/login-chat.pug
                div(class="d-none", id="insert-product-container")
                    div(class="row d-flex justify-content-center")
                        div(class="col-lg-6 col-sm-12 col-md-6 ")
                            include includes/insert-product.pug
                div(class="col-lg-12 col-sm-12 position-relative")
                    include includes/product-table.pug
    script(src="/socket.io/socket.io.js")
    script(src="//cdn.jsdelivr.net/npm/sweetalert2@11")
    script(type="text/javascript")
        include scripts/home.js 